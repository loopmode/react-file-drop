{
  "name": "@loopmode/react-file-drop",
  "author": "https://sarink.net",
  "contributors": [
    "Jovica Aleksic <jovica.aleksic@loopmode.de>"
  ],
  "version": "0.4.5",
  "description": "React component for Gmail or Facbook -like drag and drop file uploader. Drag files anywhere onto the window to highlight a 'drop area' of the page",
  "main": "dist/FileDrop/FileDrop.js",
  "types": "dist/FileDrop/FileDrop.d.ts",
  "scripts": {
    "start:dev": "webpack-dev-server",
    "build:filedrop": "tsc --project src/FileDrop",
    "build:demo": "cross-env NODE_ENV=production webpack -p",
    "lint": "tslint --project src",
    "lint:watch": "SHELL=/bin/bash chokidar webpack.config.* tslint.json tsconfig.json ./**/*.ts ./**/*.tsx -c \"npm run lint\" --initial --verbose",
    "lint:fix": "tslint --project . --fix"
  },
  "keywords": [
    "react file gmail",
    "react file facebook",
    "react file upload",
    "react file drag drop",
    "react",
    "react dropzone",
    "react-file-drop",
    "gmail",
    "facebook",
    "dropzone",
    "upload",
    "drag",
    "drop",
    "drag and drop",
    "drag n drop",
    "file",
    "window",
    "document"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/loopmode/react-file-drop.git#accept-any-drop"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loopmode/react-file-drop/issues"
  },
  "homepage": "https://github.com/loopmode/react-file-drop#readme",
  "dependencies": {
    "@loopmode/events": "1.0.4",
    "dnd-details": "^1.0.6",
    "prop-types": "^15.6.1"
  },
  "peerDependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^8.0.53",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "@types/webpack": "^3.8.1",
    "@types/webpack-dev-server": "^2.9.2",
    "chokidar": "^1.7.0",
    "chokidar-cli": "^1.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.7",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.0",
    "ts-loader": "^3.2.0",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "typescript": "^2.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
